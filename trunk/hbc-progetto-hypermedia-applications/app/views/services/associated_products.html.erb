<h1>Products that offer <%=@service.name%></h1>
<br>
<br>
<div class="gridList">
  <% @products.each do |product| %>
      <div class="gridBox">
        <%= product.name %>
        <%= link_to product_path(product) do%>
            <%= image_tag("/products/#{product.image_url}") %>
        <% end %>
      </div>
  <% end  %>
</div>