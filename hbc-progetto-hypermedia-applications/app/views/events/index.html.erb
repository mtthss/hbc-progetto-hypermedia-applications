<h1>All events</h1>

<br>
<div class="gridList">
  <% @events.each do |event| %>
      <div class="gridBox">
        &nbsp; <%= event.name %>
        <%= link_to event_path(event, :from=>"events") do%>
            <%= image_tag("/events/#{event.image_url}") %>
        <% end %>
      </div>
  <% end  %>
</div>

